{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
},
{
    "functions": {
        "router/*.tsx": {
            "memory": 1024
        },
        "router/*": {
            "maxDuration": 10
        },
        "app.js": {
            "maxDuration": 10
        }
    }
}